d1 $ s "[kick:6 kick:2]*4"

d1 $ n "[6 2]*4" # sound "kick"

d2 $ n "[1 0 [2 0] *1.5]/2" #sound "perc"

-- repetition in same speed
d2 $ sound "perc:0 perc:1!7"


d2 $ sound "perc:1(<3 4>,8)"

d3 $ sound "vocals:1(<1 8>,8)"


d3 $ fast 4 $ n "{0 4 5 6}" #sound "arpy"


d3 $ n "<[0 . 4 5] [1 2 . 4] [1 1 ~3]>*4" #sound "arpy"

d1 $ silence

draw "x(3,8)"

-- cycles per second
setcps 0.5
